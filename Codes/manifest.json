{
    "name":             "Mp3Downloader",
    "version":          "1.0.0",
    "manifest_version": 2,
    "description":      "Download mp3 from bing dictionary.",
    "content_scripts":
    [
        {
            "matches": ["http://cn.bing.com/dict/*"],
            "js":      ["Mp3Downloader.js"]
        }
    ]
}